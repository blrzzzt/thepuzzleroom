{
  "name": "thepuzzleroom.com",
  "version": "0.0.1",
  "description": "site for the puzzle room",
  "main": "index.js",
  "repository": "https://github.com/blrzzzt/thepuzzleroom",
  "author": "benjamin rabin",
  "license": "MIT",
  "private": true,
  "scripts": {
    "js": "nodemon --watch javascripts ./bin/js.js",
    "css": "nodemon --watch scss --ext scss ./bin/css.js",
    "image": "nodemon --watch images --ext png,ico,jpg,jpeg ./bin/images.js",
    "view": "nodemon --watch views --ext pug ./bin/view.js",
    "serve": "nodemon --ext pug,scss,js,json  --watch views --watch scss --watch javascripts --watch bin/server.js ./bin/server.js",
    "init": "nodemon --watch ./bin ./index.js"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "fs.extra": "^1.3.2",
    "glob": "^7.1.1",
    "imagemin": "^5.2.2",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.0",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.1",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.16",
    "pug": "^2.0.0-beta11",
    "reload": "^1.1.1",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony"
  }
}
